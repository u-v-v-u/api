{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --port 3000",
    "clean": "rm -rf .turbo && rm -rf .svelte-kit",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir --ignore-path=../../.prettierignore --check . && TIMING=1 eslint \"src\"",
    "format": "prettier --plugin-search-dir --write --ignore-path=../../.prettierignore ."
  },
  "devDependencies": {
    "@iconify/svelte": "^3.0.1",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@tailwindcss/typography": "^0.5.8",
    "@types/eslint": "^8",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.30.0",
    "postcss": "^8.4.20",
    "sass": "^1.56.2",
    "svelte": "^3.54.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^5.0.0",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "type": "module"
}
